#!nsh
#
# @name HoloFlyer Basic
#
# @type Coaxial Helicopter
# 
# @output MAIN1 Top rotor (CCW)
# @output MAIN2 Top Pitch
# @output MAIN3 Top Roll
# @output MAIN4 Bottom rotor (CW)
# @output MAIN5 Bottom Pitch
# @output MAIN6 Bottom Roll
#
# @maintainer Jimmy Paulos
#

sh /etc/init.d/rc.mc_defaults
set MIXER holoflyer_basic

if [ $AUTOCNF == yes ]
then
	param set MC_ROLL_P 6.5
	param set MC_ROLLRATE_P 0.17
	param set MC_ROLLRATE_I 0.05
	param set MC_ROLLRATE_D 0.005
	param set MC_ROLLRATE_FF 0
	param set MC_PITCH_P 6.5
	param set MC_PITCHRATE_P 0.17
	param set MC_PITCHRATE_I 0.05
	param set MC_PITCHRATE_D 0.005
	param set MC_PITCHRATE_FF 0
	param set MC_YAW_P 2
	param set MC_YAW_FF 0.5
	param set MC_YAWRATE_P 0.1
	param set MC_YAWRATE_I 0.1
	param set MC_YAWRATE_D 0.0
	param set MC_YAWRATE_FF 0
fi

echo "use PWM parameters"
set PWM_OUT 123456
set PWM_RATE 200
set PWM_DISARMED p:PWM_DISARMED
set PWM_MIN p:PWM_MIN
set PWM_MAX p:PWM_MAX